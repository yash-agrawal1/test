pipeline {
    agent any

    parameters {
        file(name: 'FILE', description: 'JSON file with config_value as a list of customer IDs')
    }

    stages {
        stage('Clean Workspace') {
            steps {
                cleanWs()
            }
        }
        stage('Read JSON and Update Items in DynamoDB') {
            steps {
                
                sh "cat ${FILE}"
            }
        }
    }
}