pipeline {
    agent any

    stages {
        stage('Clean Workspace') {
            steps {
                cleanWs()
            }
        }
        stage('Checkout Repository') {
            steps {
                git branch: 'main', url: 'https://github.com/Nykaa/custom-jobs'
            }
        }
        stage('Read JSON and Update Items in DynamoDB') {
            steps {
                
                sh "cat usm/add-items-dynamodb/config_values.json"
            }
        }
    }
}